#!/bin/bash
if [[ ! -z "$(git status --porcelain)" ]]; then
    echo "verify_clean: There are local file changes - build is not clean:"
    git status
    echo ""
    echo "Diff output:"
    git diff
    echo "--- README.md ---"
    cat ../source/js/README.md
    exit 1
fi
echo "verify_clean: OK"
exit 0
