#!/bin/bash
Fail()
{
    echo "FATAL($(realpath $0)): $1"
    exit 1
}

node positions.js +45.6 -90.7 2018-11-30T17:55:07.234Z > test/positions.txt || Fail "Error testing positions.js"
diff test/positions.txt test/positions_correct.txt || Fail "Error comparing positions.txt to positions_correct.txt"
echo "PASS: nodejs examples"
exit 0
